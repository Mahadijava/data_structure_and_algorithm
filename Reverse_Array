Array reverse by value swapping
=================================

var reversearr = function(arr){
  
  function swap(arr, start, end){
    while(start<end){
      [arr[start], arr[end]] = [arr[end], arr[start]];
      start++;
      end--;
    }
  }
  
  swap (arr, 0, (arr.length-1));
  return arr;
}

var nums = [1,2,3,4,5,6,7,8,9];
console.log(reversearr(nums));
